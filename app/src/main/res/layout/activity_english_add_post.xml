                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="12sp"
                    android:textColor="@color/green"
                    android:text="Price Unit"
                    android:visibility="gone"
                    android:layout_marginVertical="4dp"
                    android:id="@+id/land_price_type_text"
                    android:gravity="left|center"
                    tools:ignore="HardcodedText,RtlHardcoded,UnusedAttribute" />
                <com.androidbuts.multispinnerfilter.MultiSpinner
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    style="@style/spinner_style"
                    android:visibility="gone"
                    android:id="@+id/land_price_type">
                </com.androidbuts.multispinnerfilter.MultiSpinner>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone"
                android:id="@+id/contact_layout"
                android:layout_marginHorizontal="5dp"
                tools:ignore="UnusedAttribute">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:id="@+id/your_details"
                    android:visibility="gone"
                    android:textColor="@color/black"
                    android:text="Contact Details:"
                    android:background="@android:color/darker_gray"
                    tools:ignore="HardcodedText" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone"
                    android:layout_marginVertical="10dp"
                    android:id="@+id/your_details_layout">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left|center"
                        android:visibility="gone"
                        android:textColor="@color/green"
                        android:id="@+id/your_telephone"
                        android:text="Phone number"
                        tools:ignore="HardcodedText,RtlHardcoded" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/telephone_num_layout"
                        android:visibility="gone"
                        android:orientation="horizontal">
                        <com.hbb20.CountryCodePicker
                            android:id="@+id/country_code"
                            android:visibility="gone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:defaultCode="94"
                            android:layout_margin="5dp"/>
                        <EditText
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:visibility="gone"
                            android:background="@drawable/contact_border"
                            android:id="@+id/telephone_num"
                            android:gravity="left|center"
                            android:inputType="number"
                            tools:ignore="Autofill,LabelFor,RtlHardcoded" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="25dp"
                android:visibility="gone"
                android:background="@drawable/camera_button_border"
                android:id="@+id/save"
                android:text="Post Ad"
                android:textColor="@color/black"
                tools:ignore="HardcodedText" />
        </LinearLayout>
    </ScrollView>
</RelativeLayout>