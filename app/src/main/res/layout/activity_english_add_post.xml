                                    android:layout_height="20dp"
                                    android:gravity="center"
                                    android:visibility="gone"
                                    android:id="@+id/upload_btn_5"
                                    android:background="#008ba3"
                                    android:text="Upload"
                                    android:textColor="@color/black"
                                    android:layout_gravity="center"
                                    tools:ignore="HardcodedText" />
                            </LinearLayout>
                        </android.support.v7.widget.CardView>
                    </LinearLayout>
                </HorizontalScrollView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/ads_image_error"
                    android:textSize="12sp"
                    android:visibility="gone"
                    android:textColor="@android:color/holo_red_dark"
                    android:text="At least you have to include one photo."
                    tools:ignore="HardcodedText" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:visibility="gone"
                android:id="@+id/land_type_layout"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="12sp"
                    android:visibility="gone"
                    android:textColor="@color/green"
                    android:text="Land Type"
                    android:layout_marginVertical="4dp"
                    android:id="@+id/land_type_text"
                    android:gravity="left|center"
                    tools:ignore="HardcodedText,RtlHardcoded,UnusedAttribute" />
                <com.androidbuts.multispinnerfilter.MultiSpinner
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    style="@style/spinner_style"
                    android:visibility="gone"
                    android:id="@+id/land_type"
                    tools:ignore="HardcodedText">
                </com.androidbuts.multispinnerfilter.MultiSpinner>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:visibility="gone"
