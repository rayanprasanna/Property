                                android:layout_height="match_parent"
                                android:id="@+id/img_layout_2"
                                android:orientation="vertical"
                                tools:ignore="UselessLeaf">
                                <ImageView
                                    android:layout_width="90dp"
                                    android:layout_height="90dp"
                                    android:layout_margin="5dp"
                                    android:background="@drawable/ic_camera"
                                    android:scaleType="centerCrop"
                                    android:id="@+id/selected_image_2"
                                    tools:ignore="ContentDescription"
                                    android:layout_gravity="center|top"/>
                                <Button
                                    android:layout_width="wrap_content"
                                    android:layout_height="20dp"
                                    android:gravity="center"
                                    android:id="@+id/select_upload_btn_2"
                                    android:background="#008ba3"
                                    android:text="Select"
                                    android:textColor="@color/black"
                                    android:layout_gravity="center"
                                    tools:ignore="HardcodedText" />
                                <Button
                                    android:layout_width="wrap_content"
                                    android:layout_height="20dp"
                                    android:gravity="center"
                                    android:visibility="gone"
                                    android:id="@+id/upload_btn_2"
                                    android:background="#008ba3"
                                    android:text="Upload"
                                    android:textColor="@color/black"
                                    android:layout_gravity="center"
                                    tools:ignore="HardcodedText" />
                            </LinearLayout>
                        </android.support.v7.widget.CardView>
                        <android.support.v7.widget.CardView
                            android:layout_width="120dp"
                            android:layout_height="120dp"
                            app:cardCornerRadius="4dp"
                            app:cardElevation="1dp"
                            app:cardMaxElevation="2dp">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/img_layout_3"
                                android:orientation="vertical"
                                tools:ignore="UselessLeaf">
                                <ImageView
                                    android:layout_width="90dp"
                                    android:layout_height="90dp"
                                    android:layout_margin="5dp"
                                    android:background="@drawable/ic_camera"
                                    android:scaleType="centerCrop"
                                    android:id="@+id/selected_image_3"
                                    tools:ignore="ContentDescription"
                                    android:layout_gravity="center|top"/>
                                <Button
                                    android:layout_width="wrap_content"
                                    android:layout_height="20dp"
                                    android:gravity="center"
                                    android:id="@+id/select_upload_btn_3"
                                    android:background="#008ba3"
                                    android:text="Select"
                                    android:textColor="@color/black"
                                    android:layout_gravity="center"
                                    tools:ignore="HardcodedText" />
                                <Button
